  },
  "head_github": {
    "prefix": "head (github)",
    "body": [
      "/**********************************************\\",
      "*",
      "*  ${1:Project Name}",
      "*  by Andrey A. Ugolnik",
      "*  https://github.com/reybits",
      "*",
      "\\**********************************************/",
      "",
      "${2}"
    ]
  },
  "head_age": {
    "prefix": "head (age)",
    "body": [
      "/**********************************************\\",
      "*",
      "*  AGE by Andrey A. Ugolnik",
      "*  https://github.com/age-engine",
      "*",
      "\\**********************************************/",
      "",
      "${1}"
    ]
  },
  "separator": {
    "prefix": "separator",
    "body": [
      "// -----------------------------------------------------------------------------",
      ""
    ]
  },
  "once": {
    "prefix": "once",
    "body": ["#pragma once", "", ""]
  },
  "class": {
    "prefix": "class",
    "body": [
      "class ${1:ClassName}",
      "{",
      "public:",
      "    ${1:ClassName}();",
      "    ~${1:ClassName}();",
      "",
      "private:",
      "    ${2:/* data */}",
      "};"
    ]
  }
}
